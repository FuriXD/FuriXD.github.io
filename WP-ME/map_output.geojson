    "embed_snippet": "<!-- copyable -->\n<div id=\"map-container\" style=\"width:100%;height:600px;\"></div>\n<script src='https://api.mapbox.com/mapbox-gl-js/v2.15.0/mapbox-gl.js'></script>\n<link href='https://api.mapbox.com/mapbox-gl-js/v2.15.0/mapbox-gl.css' rel='stylesheet' />\n<script>\nmapboxgl.accessToken = 'YOUR_MAPBOX_TOKEN';\nconst map = new mapboxgl.Map({\n  container: 'map-container',\n  style: 'mapbox://styles/mapbox/streets-v12',\n  center: [137.5, 35.3],\n  zoom: 7\n});\n\nfetch('map_output.geojson')\n  .then(response => response.json())\n  .then(data => {\n    map.on('load', () => {\n      map.addSource('itinerary', {\n        type: 'geojson',\n        data: data\n      });\n      map.addLayer({\n        id: 'points',\n        type: 'circle',\n        source: 'itinerary',\n        paint: {\n          'circle-radius': 8,\n          'circle-color': '#FF6B6B'\n        }\n      });\n      // Add popups\n      data.features.forEach((feature, i) => {\n        const el = document.createElement('div');\n        el.className = 'marker';\n        el.textContent = feature.properties.day;\n        new mapboxgl.Marker(el)\n          .setLngLat(feature.geometry.coordinates)\n          .setPopup(new mapboxgl.Popup().setHTML(\n            `<h3>Day ${feature.properties.day}</h3>\n             <p>${feature.properties.activity}</p>\n             <small>${feature.properties.location}</small>`\n          ))\n          .addTo(map);\n      });\n    });\n  });\n</script>"
